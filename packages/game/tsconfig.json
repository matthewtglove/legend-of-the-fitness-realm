    "compilerOptions": {
        "skipLibCheck": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "lib": ["es2020", "dom"],
        "types": ["node"],
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "outDir": "dist"
    }
}
